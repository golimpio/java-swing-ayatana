<extension id="jdev.java.ayatana" version="1.2.4" esdk-version="1.0"
           rsbundle-class="jdev.java.ayatana.Res"
           xmlns="http://jcp.org/jsr/198/extension-manifest">
  <name>${EXTENSION_NAME}</name>
  <owner>Jared González</owner>
  <dependencies>
    <import>oracle.ide</import>
  </dependencies>
  <classpaths>
    <classpath>jdev.java.ayatana/jayatana-1.2.4.jar</classpath>
  </classpaths>
  <hooks>
    <feature-hook>
      <optional>true</optional>
      <description>${EXTENSION_DESC}</description>
      <copyright>Jared González</copyright>
